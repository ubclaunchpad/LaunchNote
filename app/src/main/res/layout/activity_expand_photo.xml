<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/black"
    tools:context="com.example.ubclaunchpad.launchnote.photoBrowser.ExpandPhotoActivity">

    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/expand_photo_toolbar_fragment"
        class="com.example.ubclaunchpad.launchnote.toolbar.PhotoNavigatonToolbarFragment"/>

    <android.support.v4.view.ViewPager
        android:id="@+id/expand_photo"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </android.support.v4.view.ViewPager>

</android.support.constraint.ConstraintLayout>
